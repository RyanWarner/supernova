{
  "name": "react-firebase-ssr-starter",
  "version": "1.0.0",
  "author": "Ryan Warner (https://github.com/RyanWarner)",
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "analyze": "npm run build:with-stats && npm run start:analyzer",
    "deploy:production": "npm run build:production && firebase use production && firebase deploy --only hosting,functions",
    "deploy:staging": "npm run build:staging && firebase use staging && firebase deploy --only hosting,functions",
    "build:production": "cross-env NODE_ENV=production DEPLOY_ENV=production node scripts/build.js",
    "build:staging": "cross-env NODE_ENV=production DEPLOY_ENV=staging node scripts/build.js",
    "build:with-stats": "cross-env NODE_ENV=production webpack --config config/webpack.config.js/client.prod.js --json > build/client/static/bundle-stats.json",
    "plop": "plop",
    "start": "cross-env NODE_ENV=development node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:update": "npm run test --updateSnapshot",
    "lint:fix": "eslint src/** --fix",
    "serve:firebase": "firebase serve --host 0.0.0.0 --port 3000 --only functions"
  },
  "moduleRoots": [
    "src/client",
    "src/server",
    "src/shared",
    "src",
    "node_modules"
  ],
  "dependencies": {
    "@babel/runtime": "7.5.4",
    "@tippy.js/react": "2.2.2",
    "amplitude-js": "5.2.2",
    "body-parser": "1.19.0",
    "chalk": "2.4.2",
    "connected-react-router": "6.3.1",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "deepmerge": "4.0.0",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "express-manifest-helpers": "0.6.0",
    "firebase": "6.2.1",
    "history": "4.9.0",
    "hoist-non-react-statics": "3.3.0",
    "js-cookie": "2.2.0",
    "lodash": "4.17.14",
    "react": "16.8.6",
    "react-aria-modal": "4.0.0",
    "react-device-detect": "1.6.2",
    "react-dom": "16.8.6",
    "react-ga": "2.6.0",
    "react-helmet": "5.2.1",
    "react-redux": "7.1.0",
    "react-router": "5.0.1",
    "react-router-config": "5.0.1",
    "react-router-dom": "5.0.1",
