  "name": "@yawnch/supernova",
  "version": "1.0.0",
  "author": "Ryan Warner (https://github.com/RyanWarner)",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "deploy:production": "npm run build:production && cross-env node scripts/deploy.js production",
    "deploy:staging": "npm run build:staging && cross-env node scripts/deploy.js staging",
    "build:production": "cross-env NODE_ENV=production DEPLOY_ENV=production node scripts/build.js",
    "build:staging": "cross-env NODE_ENV=production DEPLOY_ENV=staging node scripts/build.js",
    "bolt": "node scripts/bolt.js",
    "bolt:deploy": "node scripts/bolt.js && firebase deploy --only database",
    "plop": "plop",
    "storybook": "start-storybook",
    "deploy:storybook": "storybook-to-ghpages",
    "start": "cross-env NODE_ENV=development node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:update": "npm run test --updateSnapshot",
    "lint:fix": "eslint src/** --fix",
    "serve:firebase": "firebase serve --host 0.0.0.0 --port 3000 --only functions"
  },
  "moduleRoots": [
    "src/client",
    "src/server",
    "src/shared",
    "src",
    "node_modules"
  ],
