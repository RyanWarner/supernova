{
  "functions": {
  },
  "hosting": {
    "public": "public",
    "rewrites": [{
      "source": "**/**",
      "function": "app"
    }],
    "ignore": [
      "firebase.json",
      "**/node_modules/**"
    ],
    "headers": [{
      "source": "**/*.@(eot|otf|ttf|ttc|woff|woff2|font.css|js|json|map)",
      "headers": [{
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      }]
    }]
  }
}
